{
  "name": "mysql-to-zod",
  "version": "1.4.0",
  "description": "Convert MySQL schemas into Zod schemas",
  "source": "src/main.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "bin": {
    "mysql-to-zod": "dist/main.js"
  },
  "scripts": {
    "mysql-to-zod": "tsx ./src/main.ts",
    "dev": "tsx ./src/main.ts",
    "build": "zx ./build.mjs && zx ./addShebang.mjs",
    "lint-fix": "eslint --cache --fix src/ && prettier --write src/",
    "test": "jest",
    "type": "tsc --noEmit",
    "publish": "zx ./build.mjs && zx ./addShebang.mjs && npm publish"
  },
  "keywords": [
    "typescript",
    "zod",
    "mysql",
    "schema",
    "sql"
  ],
  "author": "yuumillar",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.70",
    "@swc/jest": "^0.2.27",
    "@types/eslint": "^8.44.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/prettier": "^2.7.3",
    "@types/ramda": "^0.29.3",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.15",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.1",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "camelcase": "^7.0.1",
    "commander": "^11.0.0",
    "cosmiconfig": "^8.2.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fp-ts": "^2.16.0",
    "fs-extra": "^11.1.1",
    "mysql2": "^3.5.2",
    "node-sql-parser": "^4.8.0",
    "prettier": "^3.0.0",
    "ramda": "^0.29.0",
    "ts-case-convert": "^2.0.2",
    "ts-node": "^10.9.1",
    "ts-pattern": "^5.0.4",
    "zod": "^3.21.4"
  },
  "volta": {
    "node": "18.17.0"
  }
}