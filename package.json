{
  "name": "mysql-to-zod",
  "version": "2.0.4",
  "description": "Convert MySQL schemas into Zod schemas",
  "source": "src/main.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "bin": {
    "mysql-to-zod": "dist/main.js"
  },
  "scripts": {
    "mysql-to-zod": "tsx ./src/main.ts",
    "dev": "tsx ./src/main.ts",
    "build": "zx ./build.mjs && zx ./addShebang.mjs",
    "test": "vitest",
    "type": "tsc --noEmit",
    "lint": "pnpm biome check --apply ./src",
    "fmt": "pnpm biome format --write ./src",
    "pub": "zx ./build.mjs && zx ./addShebang.mjs && npm publish"
  },
  "keywords": [
    "typescript",
    "zod",
    "mysql",
    "schema",
    "sql"
  ],
  "author": "yuumillar",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.101",
    "@swc/jest": "^0.2.29",
    "@types/eslint": "^8.56.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/prettier": "^2.7.3",
    "@types/ramda": "^0.29.9",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.20",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "knip": "^3.8.4",
    "rimraf": "^5.0.5",
    "tsx": "^3.14.0",
    "typescript": "^5.3.3",
    "vitest": "^0.33.0",
    "zod": "^3.22.4",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "@prettier/sync": "^0.3.0",
    "camelcase": "^7.0.1",
    "commander": "^11.1.0",
    "cosmiconfig": "^8.3.6",
    "cosmiconfig-typescript-loader": "^4.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fp-ts": "^2.16.1",
    "fs-extra": "^11.2.0",
    "immer": "^10.0.3",
    "mysql2": "^3.6.5",
    "node-sql-parser": "^4.17.0",
    "prettier": "^3.1.1",
    "ramda": "^0.29.1",
    "ts-case-convert": "^2.0.6",
    "ts-morph": "^20.0.0",
    "ts-node": "^10.9.2",
    "ts-pattern": "^5.0.6"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.8.1"
  }
}